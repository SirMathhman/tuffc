struct Some<T> {
    value : I32
}

struct None<T> {}

type Option<T> = Some<T> | None<T>;

fn valueOrZero(option : Option<I32>) : I32 => {
    match (option) {
        case Some { value } = value;
        case None = 0;
    }
}

fn main() : I32 => {
    let a = Some { value : 41 };
    valueOrZero(a) + 1
}